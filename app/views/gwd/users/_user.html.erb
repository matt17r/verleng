<div id="<%= dom_id user %>">
  <p>
    <strong>Person:</strong>
    <%= user.person_id %>
  </p>

  <p>
    <strong>Etag:</strong>
    <%= user.etag %>
  </p>

  <p>
    <strong>Given name:</strong>
    <%= user.given_name %>
  </p>

  <p>
    <strong>Family name:</strong>
    <%= user.family_name %>
  </p>

  <p>
    <strong>Full name:</strong>
    <%= user.full_name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Email aliases:</strong>
    <%= user.email_aliases %>
  </p>

  <p>
    <strong>Org unit:</strong>
    <%= user.org_unit %>
  </p>

  <p>
    <strong>Suspended:</strong>
    <%= user.suspended %>
  </p>

  <p>
    <strong>Deleted at:</strong>
    <%= user.deleted_at %>
  </p>

</div>
